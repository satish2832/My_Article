
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Form with Select Dropdown</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/5.2.0/css/bootstrap.min.css">
</head>
<body>

    <div class="container mt-5">
        <form>
            <div class="form-group">
                <label for="skillsetSelect">Select Skillset</label>
                <select class="form-select" id="skillsetSelect" aria-label="Select Skillset">
                    <option selected disabled>Choose Skillset</option>
                    <option value="java_fullstack">Java Fullstack</option>
                    <option value="dotnet_fullstack">.NET Fullstack</option>
                    <option value="automation_testing">Automation Testing</option>
                    <option value="manual_testing">Manual Testing</option>
                    <option value="performance_testing">Performance Testing</option>
                    <option value="cpp">C++</option>
                    <option value="servicenow">ServiceNow</option>
                    <option value="sap">SAP</option>
                    <option value="oracle_apps">Oracle Apps</option>
                    <option value="salesforce">Salesforce</option>
                    <!-- Add more skillsets as needed -->
                </select>
            </div>
        </form>
    </div>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/5.2.0/js/bootstrap.bundle.min.js"></script>
</body>
</html>

